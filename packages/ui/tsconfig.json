{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "noEmitOnError": false,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noResolve": false,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "composite": false,
    "incremental": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "../icons/**/*",
    "../theme/**/*",
    "../tokens/**/*"
  ],
  "references": [],
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
